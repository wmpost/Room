<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="650.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Room.LoggedInController">
   <children>
      <ImageView fitHeight="150.0" fitWidth="850.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Resources/logo.PNG" />
         </image>
      </ImageView>
      <Label layoutX="49.0" layoutY="220.0" prefHeight="43.0" prefWidth="756.0" text="Please select from the following options by clicking on a button below to either search for an available room to make a reservation in or manage your current reservations." textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="btnSearch" layoutX="200.0" layoutY="275.0" mnemonicParsing="false" onAction="#goToSearch" text="Search to Reserve a Room">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="btnManage" layoutX="455.0" layoutY="275.0" mnemonicParsing="false" onAction="#goToReservations" text="View &amp; Manage Reservations">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label fx:id="lblName" layoutX="325.0" layoutY="180.0" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="lblLogOut" layoutX="716.0" layoutY="74.0" onMouseEntered="#growText" onMouseExited="#shrinkText" onMousePressed="#goToLoggedOut" text="Log Out" textFill="WHITE">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <ImageView fitHeight="276.0" fitWidth="331.0" layoutX="36.0" layoutY="341.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Resources/22712216_10155820549461660_8994016111916220590_o.jpg" />
         </image>
      </ImageView>
      <ImageView fitHeight="273.0" fitWidth="435.0" layoutX="394.0" layoutY="340.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Resources/23331246_10155864113226660_2132042014537194521_o.jpg" />
         </image>
      </ImageView>
      <Label layoutX="341.0" layoutY="180.0">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
