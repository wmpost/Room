<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="650.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Room.AdminManageController">
   <children>
      <Button fx:id="btnDelRes" layoutX="329.0" layoutY="520.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="193.0" text="Delete Selected Reservation">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <ImageView fitHeight="150.0" fitWidth="850.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Resources/logo.PNG" />
         </image>
      </ImageView>
      <Label fx:id="lblLogout" layoutX="716.0" layoutY="74.0" onMouseEntered="#growText" onMouseExited="#shrinkText" onMousePressed="#goToLoggedOut" text="Log Out" textFill="WHITE">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="btnReturn" layoutX="329.0" layoutY="571.0" mnemonicParsing="false" onAction="#goToADLoggedIn" prefHeight="30.0" prefWidth="193.0" text="Return to Logged In">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label layoutX="352.0" layoutY="180.0" prefHeight="25.0" prefWidth="181.0" text="Manage all Reserverations">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TableView fx:id="tblRes" layoutX="126.0" layoutY="231.0" prefHeight="251.0" prefWidth="612.0">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
